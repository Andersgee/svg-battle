{
  "$schema": "https://json.schemastore.org/prettierrc",
  "arrowParens": "always",
  "printWidth": 120,
  "singleQuote": false,
  "jsxSingleQuote": false,
  "semi": true,
  "trailingComma": "all",
  "tabWidth": 2,
  "useTabs": false,
  "quoteProps": "consistent",
  "bracketSpacing": true,
  "bracketSameLine": false,
  "proseWrap": "preserve"
}
